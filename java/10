package matias;
import java.util.Scanner;

public class matias {

	public static void main(String[] args) {
		  Scanner scanner = new Scanner(System.in);
		 
		  int numero = 0;
		  for (int i = 1; i <= 100; i++) {
	            numero = numero + 1;
	            System.out.print(numero+".");
	}
	}
}
